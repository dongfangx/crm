<?xml version="1.0" encoding="UTF-8"?>
<ehcache updateCheck="false" monitoring="autodetect" dynamicConfig="false">

    <diskStore path="java.io.tmpdir"/>

    <defaultCache
           maxElementsInMemory="1000"
           eternal="true"
           timeToIdleSeconds="3600"
           timeToLiveSeconds="3600"
           overflowToDisk="true"
           diskSpoolBufferSizeMB="1" />

    <!-- user -->
    <cache name="user" maxElementsInMemory="1000"/>

    <!-- menu -->
    <cache name="menu" maxElementsInMemory="1000"/>

    <!-- info -->
    <cache name="info" maxElementsInMemory="1000"/>


    <!-- employee -->
    <cache name="employee" maxElementsInMemory="1000"/>

    <!-- department -->
    <cache name="department" maxElementsInMemory="1000"/>

    <!-- xinyong -->
    <cache name="xinyong" maxElementsInMemory="1000"/>

    <!-- customertype -->
    <cache name="customertype" maxElementsInMemory="1000"/>

    <!-- customerlevel -->
    <cache name="customerlevel" maxElementsInMemory="1000"/>

    <!-- linktype -->
    <cache name="linktype" maxElementsInMemory="1000"/>

    <!-- aftersalestype -->
    <cache name="aftersalestype" maxElementsInMemory="1000"/>

    <!-- unit -->
    <cache name="unit" maxElementsInMemory="1000"/>

    <!-- product -->
    <cache name="product" maxElementsInMemory="1000"/>

    <!-- customer -->
    <cache name="customer" maxElementsInMemory="1000"/>

    <!-- eventtype -->
    <cache name="eventtype" maxElementsInMemory="1000"/>

    <!-- linkcustomer -->
    <cache name="linkcustomer" maxElementsInMemory="1000"/>

    <!-- customerevent -->
    <cache name="customerevent" maxElementsInMemory="1000"/>

    <!-- linkman -->
    <cache name="linkman" maxElementsInMemory="1000"/>

    <!-- sales -->
    <cache name="sales" maxElementsInMemory="1000"/>

    <!-- aftersales -->
    <cache name="aftersales" maxElementsInMemory="1000"/>

    <!-- contract -->
    <cache name="contract" maxElementsInMemory="1000"/>


</ehcache>
